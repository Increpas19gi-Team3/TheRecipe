	<!-- 전체 데이터 -->
	<select id="selectAdminFoodListAll" resultMap="FoodJoinFoodcodeResult">
		SELECT TF.F_FDCODE, TF.E_EVTCODE, TF.FC_1ST, TF.FC_2ND, TF.FC_3RD, TFC.FC_CTGNAME, TFC.FC_ISBLOCK, 
		        TF.F_ISBLOCK, TF.F_FOODNAME, TF.F_PRICE, TF.F_EXPLAN, TF.F_IMGNAME, TF.F_THUMNAME
		FROM TR_FOOD TF 
		INNER JOIN TR_FOODCODE TFC
		ON TF.FC_1ST = TFC.FC_1ST AND TF.FC_2ND = TFC.FC_2ND AND TF.FC_3RD = TFC.FC_3RD
	</select>





	
